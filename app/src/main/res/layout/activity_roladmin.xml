<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/adminrol"
    tools:context=".Roladmin">

    <LinearLayout
        android:id="@+id/Lnusuario"
        android:layout_width="171dp"
        android:layout_height="186dp"
        android:background="@drawable/usuario"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.271"></LinearLayout>

    <LinearLayout
        android:id="@+id/Lnadmin"
        android:layout_width="171dp"
        android:layout_height="186dp"
        android:background="@drawable/gerente"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.271"></LinearLayout>

    <Button
        android:id="@+id/Badmsalir"
        android:layout_width="333dp"
        android:layout_height="83dp"
        android:text="Salir"
        android:textColorLink="#9575CD"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.945"
        tools:ignore="HardcodedText,VisualLintButtonSize" />

    <LinearLayout
        android:id="@+id/Lnvendedor"
        android:layout_width="171dp"
        android:layout_height="186dp"
        android:background="@drawable/asesor"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.678"
        tools:ignore="UselessLeaf" />
</androidx.constraintlayout.widget.ConstraintLayout>